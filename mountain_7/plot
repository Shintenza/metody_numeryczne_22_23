#!/usr/bin/gnuplot

f(x)=-0.0062 * x**7 + 0.0228 * x**6 + 0.9664 * x**5 - 1.9743 * x**4 + 1.9892 * x**3 + 0.0024 * x**2 - 5.0003 * x**1 + 1
set xrange [-1:1]
set label at 0.062500, 0.687959 "" point pointtype 7 pointsize 2
set label at 0.187500, 0.073443 "" point pointtype 7 pointsize 2
set label at 0.312500, -0.517558 "" point pointtype 7 pointsize 2
set label at 0.437500, -1.077264 "" point pointtype 7 pointsize 2
set label at 0.562500, -1.600455  "" point pointtype 7 pointsize 2
set label at 0.687500, -2.080815  "" point pointtype 7 pointsize 2
set label at 0.812500, -2.507266  "" point pointtype 7 pointsize 2
set label at 0.937500, -2.860307 "" point pointtype 7 pointsize 2
plot f(x)
pause -1
